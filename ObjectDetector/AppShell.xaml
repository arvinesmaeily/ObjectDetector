<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ObjectDetector.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:pages="clr-namespace:ObjectDetector.Pages"
    Shell.FlyoutBehavior="Disabled"
    Title="Object Detector">

    <TabBar>
        <ShellContent
            Title="Camera"
            Icon="dotnet_bot.png"
            Route="CameraPage">
            <pages:CameraPage />
        </ShellContent>

        <ShellContent
            Title="Gallery"
            Icon="dotnet_bot.png"
            Route="GalleryPage">
            <pages:GalleryPage />
        </ShellContent>

        <ShellContent
            Title="Settings"
            Icon="dotnet_bot.png"
            Route="SettingsPage">
            <pages:SettingsPage />
        </ShellContent>
    </TabBar>

</Shell>
